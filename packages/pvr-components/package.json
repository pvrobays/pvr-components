{
  "name": "pvr-components",
  "version": "0.0.2",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/pvr-components/pvr-components.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "dev": "npm-run-all --parallel stencil.watch storybook",
    "stencil.generate": "stencil generate",
    "stencil.start": "stencil build --dev --watch --serve",
    "stencil.test": "stencil test --spec --e2e",
    "stencil.test.watch": "stencil test --spec --e2e --watchAll",
    "stencil.watch": "stencil build --watch",
    "stencil.watch.dev": "stencil build --dev --watch",
    "storybook": "storybook dev -p 6006",
    "storybook.build": "storybook build"
  },
  "dependencies": {
    "@ionic/core": "^7.5.5",
    "@stencil/core": "^4.7.2",
    "@stencil/sass": "^3.0.7"
  },
  "devDependencies": {
    "@stencil/react-output-target": "^0.5.3",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.5.3",
    "@storybook/web-components": "^7.5.3",
    "@storybook/web-components-vite": "^7.5.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^18.18.9",
    "case": "^1.6.3",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "21.5.2",
    "storybook": "^7.5.3"
  },
  "license": "MIT",
  "gitHead": "d5b0ee9c9a24803616f867870751e3a8ab91bfd4"
}
